\begin_deeper
\begin_layout Block
\begin_inset Argument 2
status collapsed

\begin_layout Plain Layout
Empirical Graphex
\end_layout

\end_inset


\end_layout

\begin_layout Block
Let 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula $\widetilde{W}^{G_{s}}$
\end_inset

 be the empirical graphex of 
\begin_inset Formula $G_{s}$
\end_inset

, and define the empirical graphex 
\begin_inset Formula $\widehat{W}^{G_{s}}:[0,\frac{v_{s}}{s}]\to\{0,1\}$
\end_inset

 by 
\begin_inset Formula 
\begin{align*}
\widehat{W}^{G_{s}}(x,y) & =\widetilde{W}^{G_{s}}(\frac{v_{s}}{s}x,\frac{v_{s}}{s}y)
\end{align*}

\end_inset


\end_layout

\begin_layout Pause

\end_layout

\begin_layout Theorem
Let 
\begin_inset Formula $\widehat{G}_{r}^{(s)}$
\end_inset

 be generated by 
\begin_inset Formula $\widehat{W}^{G_{s}}(x,y)$
\end_inset

 and let 
\begin_inset Formula $\eta_{r}^{G_{s}}$
\end_inset

 be the (random, 
\begin_inset Formula $G_{s}$
\end_inset

 measurable) law of 
\begin_inset Formula $\eta^{G_{s}}$
\end_inset

.
 Then, almost surely, 
\begin_inset Formula 
\begin{align*}
\lim_{s\to\infty}\|\eta_{r}^{G_{s}}-\law(G_{r})\|_{\mathrm{TV}} & \to0,
\end{align*}

\end_inset

where 
\begin_inset Formula $\law(G_{r})$
\end_inset

 is the law of a size 
\begin_inset Formula $r$
\end_inset

 graph generated by 
\begin_inset Formula $W$
\end_inset

 and 
\begin_inset Formula $\|\cdot\|_{\mathrm{TV}}$
\end_inset

 is the total variation distance.
 
\end_layout

\begin_layout Theorem

\end_layout

\end_deeper
